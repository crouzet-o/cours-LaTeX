\setcounter{secnumdepth}{5} %% DÃ©termination du niveau d'inclusion des
%% titres dans la table des matiÃ¨res. 1 = part, 2 = chapter, 3 =
%% section, 4 = subsection, 5 = subsubsection, 6 = paragraph, 7 =
%% subparagraph


%% Pour des prÃ©sentations alternatives Ã  ce que propose LaTeX en standard

% Codage de la numÃ©rotation
%\def\thepart{\Roman{part}} % \Roman
\def\thechapter{\Roman{chapter}} % \Roman
\def\thesection{\Alph{section}}
\def\thesubsection{\Roman{subsection}} % \Alph
\def\thesubsubsection{\arabic{subsubsection}} % \Alph
\def\theparagraph{\roman{paragraph}} % \Alph
\def\thesubparagraph{\alph{subparagraph}} % \Alph

% Apparence des titres
\titleformat{\part}[display]{\raggedleft\Huge\scalefont{1.3}\sffamily\bfseries\thispagestyle{empty}}{\partname}{0cm}{}
\titleformat{\chapter}[display]{\raggedleft\Huge\sffamily\bfseries}{\chaptertitlename\ \thechapter}{0cm}{}
\titleformat{\section}{\Large\sffamily\bfseries}{\thesection)\quad}{0cm}{}
\titleformat{\subsection}{\Large\sffamily}{\thesubsection --\quad}{0cm}{}
\titleformat{\subsubsection}{\large\sffamily\itshape\raggedright}{\thesubsubsection.\quad}{0cm}{}
\titleformat{\paragraph}{\normalsize\sffamily\itshape\raggedright}{\theparagraph\quad}{0cm}{}
\titleformat{\subparagraph}{\normalsize\sffamily\itshape\raggedright}{\thesubparagraph\quad}{0cm}{}


% Codage de la numÃ©rotation
%% Pour des numÃ©rotations standard gÃ©nÃ©ralisÃ©es aux paragraphes
\def\thechapter{\arabic{chapter}} % \Roman
\def\thesection{\arabic{chapter}.\arabic{section}}
\def\thesubsection{\arabic{chapter}.\arabic{section}.\arabic{subsection}} % \Alph
\def\thesubsubsection{\arabic{chapter}.\arabic{section}.\arabic{subsection}.\arabic{subsubsection}} % \Alph
\def\theparagraph{\arabic{chapter}.\arabic{section}.\arabic{subsection}.\arabic{subsubsection}.\arabic{paragraph}} % \Alph
\def\thesubparagraph{} % \Alph

% Apparence des titres
\titleformat{\part}[display]{\raggedleft\Huge\scalefont{1.3}\sffamily\bfseries\thispagestyle{empty}}{\partname}{0cm}{}
\titleformat{\chapter}[display]{\raggedleft\Huge\sffamily\bfseries}{\chaptertitlename\ \thechapter}{0cm}{}
\titleformat{\section}{\Large\sffamily\bfseries}{\thesection\quad}{0cm}{}
\titleformat{\subsection}{\Large\sffamily}{\thesubsection\quad}{0cm}{}
\titleformat{\subsubsection}{\large\sffamily\itshape\raggedright}{\thesubsubsection\quad}{0cm}{}
\titleformat{\paragraph}{\normalsize\sffamily\itshape\raggedright}{\theparagraph\quad}{0cm}{}
\titleformat{\subparagraph}{\normalsize\sffamily\itshape\raggedright}{}{0cm}{}


